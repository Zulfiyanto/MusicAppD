{{#each musics}}
  <div class="col-md-3">
    <a href="/music/{{this.id}}">
      <div class="card" style="width: 15rem;">
        <img src="{{this.cover_music}}" class="card-img-top" alt="..." />
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <p class="card-text">{{this.name}}</p>
        </div>
      </div>
    </a>
  </div>
{{/each}}