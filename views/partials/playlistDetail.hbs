<div class="col-12 padding-none">
  {{!Nav bar }}
  {{> navbar}}
  <div class="top-track">
    <div class="top">
    <h2>Playlist</h2>
     <div class="btn-register mb-2">
        <button  type="button"
        data-bs-toggle="modal"
        data-bs-target="#playlist">Add</button>
      </div>
    </div>
    <div class="row mt-5">
      {{#each playlists}}
  <div class="col-md-3">
    <a href="">
      <div class="card" style="width: 15rem;">
        <img src="{{this.cover_music}}" class="card-img-top" alt="..." />
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <p class="card-text">{{this.name}}</p>
        </div>
      </div>
    </a>
  </div>
{{/each}}
    </div>
  </div>
</div>